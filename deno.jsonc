{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "test": "deno test --allow-read --allow-write --allow-env --allow-net",
    "recache": "deno cache -r --lock=deno.lock --lock-write ./main.ts",
    "esm:add": "deno run -A https://esm.sh/v124 add",
    "esm:update": "deno run -A https://esm.sh/v124 update",
    "esm:remove": "deno run -A https://esm.sh/v124 remove",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "@core/": "./core/",
    "@home/": "./domains/home/",
    "@og/": "./domains/og/",
    "@post/": "./domains/post/",
    "@privacy/": "./domains/privacy/",
    "@sitemap/": "./domains/sitemap/",
    "@islands/": "./islands/",
    "@shared/": "./shared/",
    "octokit": "https://cdn.skypack.dev/octokit@2.0.14?dts",
    "hljs": "https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.7.0/es/highlight.min.js",
    "std/": "https://deno.land/std@0.185.0/",
    "$fresh/": "https://deno.land/x/fresh@1.4.2/",
    "i18next": "https://deno.land/x/i18next@v22.4.15/index.js",
    "resvg/": "https://deno.land/x/resvg_wasm@0.2.0/",
    "zod": "https://deno.land/x/zod@v3.21.4/mod.ts",
    "xml": "https://deno.land/x/xml@2.1.1/mod.ts",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "stitches": "https://esm.sh/v124/@stitches/core@1.2.8",
    "stitches/": "https://esm.sh/v124/@stitches/core@1.2.8/",
    "clsx": "https://esm.sh/v124/clsx@1.2.1",
    "dayjs": "https://esm.sh/v124/dayjs@1.11.7",
    "dayjs/": "https://esm.sh/v124/dayjs@1.11.7/",
    "parse5": "https://esm.sh/v124/parse5@7.1.2",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.1",
    "preact": "https://esm.sh/preact@10.15.1",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "satori": "npm:satori@0.8.0"
  },
  "scopes": {
    "https://esm.sh/v124/": {
      "entities": "https://esm.sh/v124/entities@4.5.0",
      "pretty-format": "https://esm.sh/v124/pretty-format@3.8.0"
    }
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    },
    "exclude": [
      "_fresh"
    ]
  },
  "fmt": {
    "exclude": [
      "_fresh"
    ]
  }
}
