{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": ["esnext"],
    "types": ["jest"],
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noEmit": true
  }
}
