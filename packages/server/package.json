{
  "name": "@mya-ake-com/server",
  "version": "1.0.0",
  "description": "portfolio server",
  "author": "mya-ake <make.app.ts@gmail.com> (https://github.com/mya-ake)",
  "homepage": "https://github.com/mya-ake/portfolio#readme",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mya-ake/portfolio.git"
  },
  "scripts": {
    "dev": "ts-node-dev --transpile-only src/local-start.ts",
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "start": "node dist/local-start.js",
    "lint": "eslint --config ./.eslintrc.js .",
    "lint:fix": "eslint --config ./.eslintrc.js --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:unit": "jest",
    "deploy:dev": "SERVER_STAGE=dev sls deploy",
    "deploy:prod": "SERVER_STAGE=prod sls deploy"
  },
  "bugs": {
    "url": "https://github.com/mya-ake/portfolio/issues"
  },
  "dependencies": {
    "@mya-ake-com/graphql-schema": "^1.0.0",
    "apollo-datasource-rest": "^0.13.0",
    "apollo-server-lambda": "^2.24.0",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.0"
  },
  "devDependencies": {
    "apollo-server-express": "^2.24.0",
    "express": "^4.17.1",
    "serverless": "^2.42.0",
    "serverless-plugin-monorepo": "^0.10.0",
    "ts-node-dev": "^1.1.6"
  }
}
