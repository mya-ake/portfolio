{
  "name": "@mya-ake-com/server",
  "version": "1.0.0",
  "description": "portfolio server",
  "author": "mya-ake <make.app.ts@gmail.com> (https://github.com/mya-ake)",
  "homepage": "https://github.com/mya-ake/portfolio#readme",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mya-ake/portfolio.git"
  },
  "scripts": {
    "dev": "ts-node-dev --transpile-only src/local-start.ts",
    "dev:mock": "USE_MOCK=true run-s dev",
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "start": "node dist/local-start.js",
    "start:mock": "USE_MOCK=true run-s start",
    "lint": "eslint --config ./.eslintrc.js .",
    "lint:fix": "eslint --config ./.eslintrc.js --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:unit": "vitest run",
    "deploy:dev": "SERVER_STAGE=dev sls deploy",
    "deploy:prod": "SERVER_STAGE=prod sls deploy"
  },
  "bugs": {
    "url": "https://github.com/mya-ake/portfolio/issues"
  },
  "dependencies": {
    "@graphql-tools/mock": "^8.1.3",
    "@graphql-tools/schema": "^8.0.0",
    "@graphql-tools/utils": "^8.0.0",
    "@mya-ake-com/error": "^1.0.0",
    "@mya-ake-com/graphql-schema": "^1.0.0",
    "apollo-datasource-rest": "3.6.0",
    "apollo-server-core": "3.8.2",
    "apollo-server-express": "3.9.0",
    "apollo-server-lambda": "3.9.0",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "graphql": "^16.5.0"
  },
  "devDependencies": {
    "@mya-ake-com/mock": "^1.0.0",
    "@reduxjs/toolkit": "^1.8.1",
    "redux-thunk": "^2.4.1",
    "serverless": "^3.18.1",
    "serverless-plugin-monorepo": "^0.11.0",
    "ts-node-dev": "^2.0.0"
  }
}
